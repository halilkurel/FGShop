@model List<ImageResult>

<div class="col-xxl-4 col-md-6 box-col-12">
    <div class="card">
        <div class="card-body">
            <div class="product-slider owl-carousel owl-theme" id="sync1">
                @foreach (var item in Model)
                {
                    <div class="item"><img src="@item.ImageUrl" alt=""></div>
                }
            </div>
            <div class="owl-carousel owl-theme" id="sync2">
                @foreach (var item2 in Model)
                {
                    <div class="item"><img src="@item2.ImageUrl" alt=""></div>
                }
            </div>
        </div>
        <a href="@Url.Action("UpdateProducthasImages", "ProducthasImage", new { area = "Admin", id = ViewBag.Id })" class="btn btn-primary mt-2">Resim Dosyalarını Güncelle</a>
    </div>
</div>
